abstract car
    xor transmission
        automatic
        manual
    `fca ?
    `cc
    [acc => fca]

abstract fca
    xor sensor
        radar
        lidar

abstract cc
    switches 2..3
        backlight ?
    acc ?

abstract camaro : car
    [all s : switches | s.backlight]
    transformer ?
        [ # switches = 3 ]

bumbleBee : camaro
    [ transformer ]
    [ manual ]
